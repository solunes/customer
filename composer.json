{
    "name": "solunes/customer",
    "description": "Paquete adicional de Clientes para proyectos de Solunes.",
    "keywords": ["laravel", "solunes", "todotix", "customer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Solunes Digital",
            "email": "info@solunes.com"
        }
    ],
    "require": {
        "solunes/master": "*",
        "solunes/notification": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/database/seeds",
            "src/app",
            "src/app/Controllers",
            "src/app/Console",
            "src/app/Helpers",
            "src/app/Listeners",
            "src/app/Providers",
            "src/app/Routes"
       ],
        "psr-4": {
            "Solunes\\Customer\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true
}
